@page "/profile/settings"
@inject IIndexedDbAccessor IndexedDbAccessor

<DrilldownOptions jsonString="@json" />

@code {
    string json = "";
    protected async override void OnInitialized()
    {
        base.OnInitialized();
        json = await jsr.InvokeAsync<string?>("localStorage.getItem", "settings").ConfigureAwait(false);
    }
}